          <TableColumnHeader>
            <Width>20</Width>
          </TableColumnHeader>
          <TableColumnHeader>
            <Width>60</Width>
          </TableColumnHeader>
          <TableColumnHeader/>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>Name</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Value</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Breadth</PropertyName>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
      </TableControl>
    </View>
    <View>
      <Name>NameAndDescription</Name>
      <ViewSelectedBy>
        <TypeName>Octopus.Client.Model.ProjectResource</TypeName>
        <TypeName>Octopus.Client.Model.ProjectGroupResource</TypeName>
        <TypeName>Octopus.Client.Model.LibraryVariableSetResource</TypeName>
        <TypeName>Octopus.Client.Model.EnvironmentResource</TypeName>
